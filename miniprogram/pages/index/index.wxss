:host {
  background: #f7f7f7;
}

.navbar-container {
  width: 100%;
  display: flex;
  align-items: center;
}

.rank-picker {
  padding: 0;
  margin: 0;
}

.navbar-options-images {
  width: 60rpx;
  height: 60rpx;
  margin-left: 20rpx;
}

.container {
  width: 100vw;
}

.area {
  width: 100%;
  background: #f7f7f7;
}

.item {
  width: 100%;
  background: #ffffff;
}

.item-container {
  height: 100%;
  display: flex;
  padding: 30rpx;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
}

.star-container {
  display: flex;
  align-items: center;
}

.star {
  width: 40rpx;
  height: 40rpx;
}

.right {
  display: flex;
  align-items: center;
}

.options-images {
  width: 40rpx;
  height: 40rpx;
  margin-left: 20rpx;
}


.options-add {
  border-radius: 50%;
  width: 130rpx;
  height: 130rpx;
  position: fixed;
  bottom: 50rpx;
  right: 30rpx;
  z-index: 999;
  background: #ffffff;
}